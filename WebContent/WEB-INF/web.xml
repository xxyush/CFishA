  </servlet>
  <servlet-mapping>
    <servlet-name>getInitialData</servlet-name>
    <url-pattern>/getInitialData</url-pattern>
  </servlet-mapping>
  
 
  
  
  <session-config>
    <session-timeout>60</session-timeout>
  </session-config>
  <welcome-file-list>
    <welcome-file>./jsp/login/login.jsp</welcome-file>
  </welcome-file-list>
</web-app>