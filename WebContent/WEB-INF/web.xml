  </servlet>
  <servlet-mapping>
    <servlet-name>getFpgaUsed</servlet-name>
    <url-pattern>/getFpgaUsed</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>getPower</servlet-name>
    <servlet-class>CFA.resourceDisplay.getPower</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getPower</servlet-name>
    <url-pattern>/getPower</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>getCpuComponentPower</servlet-name>
    <servlet-class>CFA.resourceDisplay.getCpuComponentPower</servlet-class>
