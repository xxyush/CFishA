    <url-pattern>/getFpgaPower</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>getCabinetPower</servlet-name>
    <servlet-class>CFA.resourceDisplay.getCabinetPower</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getCabinetPower</servlet-name>
    <url-pattern>/getCabinetPower</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>getInitialData</servlet-name>
